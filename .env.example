# 模型配置
# 供应商支持: google, openai, anthropic, deepseek
LLM_PROVIDER=google
MODEL_NAME=gemini-2.0-flash

# 各供应商 API Key
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1
ANTHROPIC_API_KEY=your_anthropic_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_BASE=https://api.deepseek.com

# Tavily 搜索 API Key (选填，无则使用浏览器搜索替代)
TAVILY_API_KEY=your_tavily_api_key_here

# 数据库路径 (选填，默认 data/work.db)
DB_PATH=data/work.db

# 日志级别 (选填，默认 INFO)
LOG_LEVEL=INFO

# 安全配置
MASTER_WXID=your_master_wxid_here
MASTER_REMARK=your_master_remark_here
# 代理配置 (如果你在中国大陆使用，通常需要配置此项)
# 示例: http://127.0.0.1:7890 (请根据你的代理软件实际端口填写)
HTTPS_PROXY=
HTTP_PROXY=

# 微信白名单，用逗号分隔
WHITELIST=文件传输助手,张三,李四
